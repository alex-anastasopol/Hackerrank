<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="pieReport" pageWidth="350" pageHeight="842"  isIgnorePagination="true" columnWidth="250" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<style name="Arial_Normal" isDefault="false" isStyledText="true" isBlankWhenNull="true" fontName="Arial" fontSize="12" pdfFontName="Helvetica" pdfEncoding="Cp1252"/>
	<style name="Arial_Bold" isDefault="false" isStyledText="true" isBlankWhenNull="true" fontName="Arial" fontSize="12" isBold="true" pdfFontName="Helvetica" pdfEncoding="Cp1252"/>
	<parameter name="reportTitle" class="java.lang.String"/>
	<parameter name="reportParameters" class="java.lang.String"/>
	<parameter name="reportDate" class="java.lang.String"/>
	<parameter name="intervalDescription" class="java.lang.String"/>
	<parameter name="titleName" class="java.lang.String"/>
	<parameter name="total" class="java.lang.String"/>
	<parameter name="titleValue" class="java.lang.String"/>
	<field name="name" class="java.lang.String"/>
	<field name="value" class="java.lang.String"/>
	
	<title>
		<band height="160">
			<textField>
				<reportElement style="Arial_Bold" x="0" y="0" width="350" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ATS " + $P{reportTitle}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement style="Arial_Normal" x="0" y="15" width="350" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{reportDate}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement style="Arial_Normal" x="0" y="30" width="350" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Total Number of Orders: " + $P{total}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement style="Arial_Normal" x="0" y="45" width="350" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{intervalDescription}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal" stretchType="RelativeToBandHeight" x="0" y="60" width="350" height="60"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{reportParameters}]]></textFieldExpression>
			</textField>
			
		</band>
	</title>
	<pageHeader>
		<band height="20" >
		 <!--1-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="0" y="1" width="175" height="18" mode="Transparent"/>
				
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{titleName}]]></textFieldExpression>
			</textField>
		 <!--2-->
			
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="175" y="1" width="175" height="18" mode="Transparent"/>
				
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{titleValue}]]></textFieldExpression>
			</textField>
		 
						
		</band>
	</pageHeader>
	
	<detail>
		<band height="25">
		 <!--1-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal" stretchType="RelativeToBandHeight" x="0" y="0" width="175" height="25" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single" >
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{name}]]></textFieldExpression>
			</textField>
		 <!--2-->
							
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal" stretchType="RelativeToBandHeight" x="175" y="0" width="175" height="25" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single" isStyledText="true">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{value}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
