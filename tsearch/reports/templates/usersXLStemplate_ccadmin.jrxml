<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="UsersReport" pageWidth="2000" pageHeight="970"  isIgnorePagination="true" orientation="Landscape" columnWidth="1800" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<style name="Arial_Normal" isDefault="false" isStyledText="true" isBlankWhenNull="true" fontName="Arial" fontSize="12" pdfFontName="Helvetica" pdfEncoding="Cp1252" />
	<style name="Arial_Normal_Hidden"  isDefault="false" isStyledText="true" isBlankWhenNull="true" fontName="Arial" fontSize="12" pdfFontName="Helvetica" pdfEncoding="Cp1252" >
		<conditionalStyle>
        	<conditionExpression><![CDATA[Boolean.valueOf($F{userHidden})]]></conditionExpression>
        	<style forecolor="red" />
    	</conditionalStyle>
    
	</style>
	<style name="Arial_Bold" isDefault="false" isStyledText="true" isBlankWhenNull="true" fontName="Arial" fontSize="12" isBold="true" pdfFontName="Helvetica" pdfEncoding="Cp1252" />

	<parameter name="CommunityName" class="java.lang.String"/>
	<parameter name="HiddenStatus" class="java.lang.String"/>
	<parameter name="Roles" class="java.lang.String"/>
	<parameter name="TotalUsers" class="java.lang.String"/>
	<parameter name="FindUser" class="java.lang.String"/>

	<field name="userLastName" class="java.lang.String"/>
	<field name="userFirstName" class="java.lang.String"/>
	<field name="userLoginName" class="java.lang.String"/>
	<field name="userRole" class="java.lang.String"/>
	<field name="userCompanyName" class="java.lang.String"/>
	<field name="userLastLogin" class="java.lang.String"/>
	<field name="userEmailAddress" class="java.lang.String"/>
	<field name="userHidden" class="java.lang.Boolean"/>
	
	<title>
		<band height="145">
			<textField>
				<reportElement style="Arial_Bold" x="0" y="0" width="990" height="40"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="20"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Users Report"]]>
				</textFieldExpression>
			</textField>
			
			<textField>
				<reportElement style="Arial_Normal" x="0" y="60" width="990" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Users from community: " + $P{CommunityName}]]>
				</textFieldExpression>
			</textField>
			
			<textField>
				<reportElement style="Arial_Normal" x="0" y="75" width="990" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["With role: "+ $P{Roles}]]>
				</textFieldExpression>
			</textField>
			
			<textField>
				<reportElement style="Arial_Normal" x="0" y="90" width="990" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Hidden status: "+ $P{HiddenStatus}]]>
				</textFieldExpression>
			</textField>
			
			<textField>
				<reportElement style="Arial_Normal" x="0" y="105" width="990" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Total Number of users: "+ $P{TotalUsers}]]>
				</textFieldExpression>
			</textField>
			
			<textField>
				<reportElement style="Arial_Normal" x="0" y="120" width="990" height="15"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Containing word: "+ $P{FindUser}]]>
				</textFieldExpression>
			</textField>
		</band>
	</title>
	
	<pageHeader>
		<band height="50">
			<!--1-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="0" y="1" width="150" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Last Name"]]>
				</textFieldExpression>
			</textField>
			
			<!--2-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="150" y="1" width="150" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["First Name"]]>
				</textFieldExpression>
			</textField>

			<!--3-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="300" y="1" width="150" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Login Username"]]>
				</textFieldExpression>
			</textField>
			
			<!--4-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="450" y="1" width="80" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Role"]]>
				</textFieldExpression>
			</textField>
			
			<!--5-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="530" y="1" width="150" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Company Name"]]>
				</textFieldExpression>
			</textField>
			
			<!--6-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="680" y="1" width="120" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Last login"]]>
				</textFieldExpression>
			</textField>
			
			<!--7-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Bold" stretchType="RelativeToBandHeight" x="800" y="1" width="190" height="40" mode="Transparent"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA["Email Address"]]>
				</textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	
	<detail>
		<band height="50" >
			<!--1-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="0" y="0" width="150" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single" >
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userLastName}]]>
				</textFieldExpression>
			</textField>
			
			<!--2-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="150" y="0" width="150" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userFirstName}]]>
				</textFieldExpression>
			</textField>
			
			<!--3-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="300" y="0" width="150" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userLoginName}]]>
				</textFieldExpression>
			</textField>
			
			<!--4-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="450" y="0" width="80" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userRole}]]>
				</textFieldExpression>
			</textField>
			
			<!--5-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="530" y="0" width="150" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userCompanyName}]]>
				</textFieldExpression>
			</textField>
			
			<!--6-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="680" y="0" width="120" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userLastLogin}]]>
				</textFieldExpression>
			</textField>
			
			<!--7-->
			<textField isStretchWithOverflow="true">
				<reportElement style="Arial_Normal_Hidden" stretchType="RelativeToBandHeight" x="800" y="0" width="190" height="40" isRemoveLineWhenBlank="true" isPrintInFirstWholeBand="true"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String">
					<![CDATA[$F{userEmailAddress}]]>
				</textFieldExpression>
			</textField>
		</band>
	</detail>
	
</jasperReport>